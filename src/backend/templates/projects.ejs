<body>
  <h2>Projects</h2>
  <ul>
    <% for (var i = 0; i < projects.length; i++) { %>
    <li>
      <%= projects[i].name %>
      <ul>
      <% for (var j = 0; j < projects[i].langs.length; j++) { %>
        <li>
          <a href="/<%= projects[i].name %>/<%= projects[i].langs[j] %>">
            <%= projects[i].langs[j] %>
          </a>
        </li>
      <% } %>
      </ul>
      <br/>
    </li>
    <% } %>
  </ul>
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async="" src="https://www.googletagmanager.com/gtag/js?id=UA-108227328-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-108227328-1');
  </script>
</body>