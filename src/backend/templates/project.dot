<body>
  <h2>Commits</h2>
  <ul>
    {{~it.commits:commit}}
    <li>
      {{?it.commit == commit }}<b>{{?}}
      <a href='/{{=it.project}}/{{=commit.sha()}}'>
        {{=commit.message()}}
      </a>
      {{?it.commit == commit}}</b>{{?}}
    </li>
    {{~}}
  </ul>

  <h2>Files</h2>
  <ul>
    {{~it.files:file}}
    <li>{{=file}}</li>
    {{~}}
  </ul>
</body>