<head>
  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/tomorrow-night.min.css">
</head>

<body>
  <h2>Commits</h2>
  <ul>
    {{~it.commits:commit}}
    <li>
      {{?it.commit == commit }}<b>{{?}}
      <a href='/{{=it.project}}/{{=commit.sha()}}'>
        {{=commit.message()}}
      </a>
      {{?it.commit == commit}}</b>{{?}}
    </li>
    {{~}}
  </ul>

  <h2>Files</h2>
  <ul>
    {{~it.files:file}}
    <li>{{=file.name}}</li>
    {{~}}
  </ul>

  <h2>File</h2>
  <pre>
    <code>{{=it.files[1].body}}</code>
  </pre>

  <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js"></script>
  <script>
    hljs.initHighlightingOnLoad();
  </script>
</body>